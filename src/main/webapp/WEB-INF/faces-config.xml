<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    
    <application>
        <resource-bundle>
            <base-name>/bundle/label</base-name>
            <var>label</var>
        </resource-bundle>
    </application>
    
    <application>
        <resource-bundle>
            <base-name>/bundle/botao</base-name>
            <var>botao</var>
        </resource-bundle>
    </application>
    
    <application>
        <resource-bundle>
            <base-name>/bundle/message</base-name>
            <var>message</var>
        </resource-bundle>
    </application>
    
    <application>
        <resource-bundle>
            <base-name>/bundle/menu</base-name>
            <var>menu</var>
        </resource-bundle>
    </application>

</faces-config>
